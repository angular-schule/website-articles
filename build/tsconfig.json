{
  "compilerOptions": {
    "lib": ["es2023"],
    "module": "commonjs",
    "moduleResolution": "bundler",
    "target": "es2023",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitOnError": false,
    "noImplicitAny": true
  }
}
